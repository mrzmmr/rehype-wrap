{
  "name": "rehype-wrap",
  "version": "1.0.0",
  "description": "wrap selected elements with a given element",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf test.js index.js coverage .nyc_output",
    "pretest": "npm run babel",
    "tap": "tap test.js",
    "babel": "babel src -d .",
    "test": "nyc --reporter=lcov npm run tap",
    "nyc": "nyc --reporter=html tap test.js -Rspec"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrzmmr/rehype-wrap.git"
  },
  "keywords": [
    "rehype",
    "plugin",
    "wrap",
    "wrapper",
    "container"
  ],
  "author": "mrzmmr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrzmmr/rehype-wrap/issues"
  },
  "homepage": "https://github.com/mrzmmr/rehype-wrap#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "nyc": "^11.3.0",
    "rehype": "^5.0.0",
    "rehype-stringify": "^3.0.0",
    "remark-parse": "^4.0.0",
    "remark-rehype": "^2.0.1",
    "tap": "^10.7.3",
    "tape": "^4.8.0",
    "unified": "^6.1.6"
  },
  "dependencies": {
    "hast-util-parse-selector": "^2.1.0",
    "hast-util-select": "^1.0.1",
    "unist-util-visit": "^1.2.0"
  }
}
